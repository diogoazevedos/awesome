{
	"format_on_save": "off",
	"hard_tabs": true,
	"tab_size": 4,
	"preferred_line_length": 100,
	"wrap_guides": [100, 120],
	"show_whitespaces": "all",
	"colorize_brackets": true,
	"git": {
		"inline_blame": {
			"show_commit_summary": true
		}
	},
	"calls": {
		"mute_on_join": true
	},
	"agent": {
		"default_model": {
			"provider": "copilot_chat",
			"model": "claude-sonnet-4.5"
		},
		"inline_assistant_model": {
			"provider": "copilot_chat",
			"model": "gpt-5.2-codex"
		},
		"commit_message_model": {
			"provider": "copilot_chat",
			"model": "gpt-5-mini"
		},
		"thread_summary_model": {
			"provider": "copilot_chat",
			"model": "gpt-5-mini"
		},
		"inline_alternatives": [
			{
				"provider": "copilot_chat",
				"model": "claude-opus-4.6"
			}
		],
		"favorite_models": [
			{
				"provider": "copilot_chat",
				"model": "claude-sonnet-4.5"
			},
			{
				"provider": "copilot_chat",
				"model": "gpt-5.2"
			},
			{
				"provider": "copilot_chat",
				"model": "gemini-3-pro-preview"
			},
			{
				"provider": "copilot_chat",
				"model": "claude-opus-4.6"
			},
			{
				"provider": "copilot_chat",
				"model": "gpt-5-mini"
			}
		]
	},
	"edit_predictions": {
		"provider": "copilot"
	},
	"theme": "Dracula",
	"base_keymap": "Atom",
	"buffer_font_family": "JetBrains Mono",
	"buffer_font_size": 13.0,
	"terminal": {
		"font_family": "Monaco",
		"minimum_contrast": 0.0
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	}
}
